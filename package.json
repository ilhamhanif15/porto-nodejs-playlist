    "mongoose-unique-validator": "^3.0.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "nodemon": "^2.0.15"
